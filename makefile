ABC.exe:main.o big2.o fact.o big3.o fibo.o pali.o rev.o sort.o sum.o 
	gcc -o ABC.exe main.o big2.o fact.o big3.o fibo.o pali.o rev.o sort.o sum.o

main.o:main.c
	gcc -c main.c
big2.o:big2.c
	gcc -c big2.c
fact.o:fact.c
	gcc -c fact.c
big3.o:big3.c
	gcc -c big3.c
fibo.o:fibo.c
	gcc -c fibo.c
pali.o:pali.c
	gcc -c pali.c
rev.o:rev.c
	gcc -c rev.c
sort.o:sort.c
	gcc -c sort.c
sum.o:sum.c
	gcc -c sum.c
clean:
	rm -rf *.o ABC.exe
